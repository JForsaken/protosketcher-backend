{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_doc"
          ]
        }
      ]
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
